<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sensorsdata.analytics.android.demo">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TestMainActivity"
            android:theme="@style/AppThemeCompat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="debugmode"
                    android:scheme="sa8a9ab067" />
            </intent-filter>
        </activity>

        <activity
            android:name=".BasePropertyActivity"
            android:theme="@style/AppThemeCompat" />

        <activity
            android:name=".TrackProfileSettingsActivity"
            android:theme="@style/AppThemeCompat" />

        <activity
            android:name=".H5Activity"
            android:theme="@style/AppThemeCompat" />

        <activity
            android:name=".WidgetTestActivity"
            android:theme="@style/AppThemeCompat" />

        <activity
            android:name=".DataBindingLambdaActivity"
            android:theme="@style/AppThemeCompat" />

        <activity
            android:name=".ViewPagerTestActivity"
            android:theme="@style/AppThemeCompat" />

        <activity
            android:name=".ViewScreenActivity"
            android:theme="@style/AppThemeCompat" />

        <activity
            android:name=".ViewScreenIgnoreActivity"
            android:theme="@style/AppThemeCompat" />

        <activity
            android:name=".ListViewTestActivity"
            android:theme="@style/AppThemeCompat" />

        <meta-data
            android:name="com.sensorsdata.analytics.android.EnableLogging"
            android:value="true" />
    </application>

</manifest>